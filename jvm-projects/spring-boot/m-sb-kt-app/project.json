{
  "name": "m-sb-kt-app",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "./jvm-projects/spring-boot/m-sb-kt-app/src",
  "targets": {
    "build": {
      "executor": "@jnxplus/nx-maven:run-task",
      "outputs": ["{projectRoot}/target", "{projectRoot}/.flattened-pom.xml"],
      "options": {
        "task": "package spring-boot:repackage -DskipTests=true"
      }
    },
    "build-image": {
      "executor": "@jnxplus/nx-maven:run-task",
      "options": {
        "task": "spring-boot:build-image"
      }
    },
    "serve": {
      "executor": "@jnxplus/nx-maven:run-task",
      "options": {
        "task": "spring-boot:run"
      },
      "dependsOn": ["^build"]
    },
    "test": {
      "executor": "@jnxplus/nx-maven:run-task",
      "options": {
        "task": "test"
      },
      "dependsOn": ["^build"]
    },
    "ktlint": {
      "executor": "@jnxplus/nx-maven:run-task",
      "options": {
        "task": "antrun:run@ktlint"
      },
      "dependsOn": ["^build"]
    },
    "ktlint-format": {
      "executor": "@jnxplus/nx-maven:run-task",
      "options": {
        "task": "antrun:run@ktlint-format"
      },
      "dependsOn": ["^build"]
    }
  },
  "tags": []
}
